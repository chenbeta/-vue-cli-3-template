.f-l
  float left

.f-r
  float right

.limit-width
  width 1200px
  margin auto

li
  list-style none

body
  font-family 'pingFangSC-Regular', sans-serif

ul, p, pre
  margin 0
  padding 0

pre
  white-space pre-wrap
  word-break break-all

.clearfix
  zoom 1

  &:after
    content ''
    clear both
    height 0
    line-height 0
    display block
    visibility hidden

[class^='el-icon-bank'], [class*=' el-icon-bank']
  font-family 'iconfont' !important
  font-size 16px
  font-style normal
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
